<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="view_helpdesk_subject_area_form" model="ir.ui.view">
        <field name="name">helpdesk.ticket.subject.area.form</field>
        <field name="model">helpdesk.ticket.subject.area</field>
        <field name="arch" type="xml">
            <form string="Subject Area">
                <sheet>
                    <field name="name"/>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_helpdesk_subject_area_tree" model="ir.ui.view">
        <field name="name">helpdesk.ticket.subject.area.tree</field>
        <field name="model">helpdesk.ticket.subject.area</field>
        <field name="arch" type="xml">
            <tree string="Subject Area">
                <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>
